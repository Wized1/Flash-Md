    "CHAT_BOT": {
      "description": "Set to 'on' if you want the bot to respond to user messages automatically. Use 'off' to disable auto-chatting.",
      "value": "off",
      "required": false
    },
    "STARTING_MESSAGE": {
      "description": "on",
      "value": "on",
      "required": true
    },
    "PRESENCE": {
      "description": "online",
      "value": "",
      "required": false
    }
